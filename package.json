{
  "name": "agent-slither",
  "version": "1.0.0",
  "description": "A slither.io-style game arena for AI agents",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/server/index.ts",
    "build": "tsc && vite build && npm run build:claw-shooter",
    "build:claw-shooter": "vite build --config vite.claw-shooter.config.ts",
    "start": "node dist/server/index.js",
    "build:client": "vite build",
    "build:reown": "vite build --config vite.reown.config.ts",
    "migrate": "tsx scripts/migrate.ts",
    "backup:db": "tsx scripts/backup-db.ts",
    "normalize-winner-names": "tsx scripts/normalize-winner-names.ts",
    "house-bots": "node scripts/run-house-bots.js",
    "shooter-house-bots": "node scripts/run-shooter-house-bots.js",
    "upload-nft-images": "tsx scripts/upload-nft-images-to-r2.ts",
    "upload-nft-metadata": "tsx scripts/upload-nft-metadata-to-r2.ts",
    "sample-nft-image": "tsx scripts/generate-sample-nft-image.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.990.0",
    "@dimforge/rapier3d-compat": "^0.19.3",
    "@gltf-transform/core": "^4.3.0",
    "@react-three/drei": "^9.103.0",
    "@react-three/fiber": "8.13.3",
    "@react-three/postprocessing": "^2.16.2",
    "@react-three/rapier": "^1.3.0",
    "@reown/appkit": "^1.8.17",
    "@reown/appkit-adapter-ethers": "^1.8.17",
    "cors": "^2.8.5",
    "ethers": "^6.16.0",
    "express": "^4.18.2",
    "pg": "^8.11.5",
    "playroomkit": "^0.0.71",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "skia-canvas": "^3.0.8",
    "socket.io": "^4.7.4",
    "socket.io-client": "^4.8.3",
    "three": "0.160.0",
    "three-stdlib": "^2.29.6"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.5",
    "@types/pg": "^8.16.0",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@types/three": "0.160.0",
    "@vitejs/plugin-react": "^3.1.0",
    "autoprefixer": "^10.4.19",
    "dotenv": "^17.2.4",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.3",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vite": "4.5.0"
  }
}
